<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.max.aicomplier.ImageAdjust">
    <Button
        android:id="@+id/back"
        android:layout_width="50dp"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:drawableEnd="@android:drawable/ic_media_previous"
        android:drawableRight="@android:drawable/ic_media_previous"
        android:layout_alignParentLeft="true" />
    <com.example.max.aicomplier.TouchView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/touch"/>
<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/display"/>
    <Button
        android:id="@+id/next"
        android:layout_width="50dp"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:drawableEnd="@android:drawable/ic_media_next"
        android:drawableRight="@android:drawable/ic_media_next" />


</RelativeLayout>
